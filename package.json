{
  "name": "verger",
  "version": "0.2.1",
  "description": "Generation of AST types and helper functions",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "tsc",
    "sample": "yarn sample:expr && yarn sample:repmin && yarn sample:kitchen && yarn sample:spec && yarn sample:ir",
    "sample:expr": "ts-node scripts/verger.ts samples/expr.yaml samples/expr.ts",
    "sample:repmin": "ts-node scripts/verger.ts samples/repmin.yaml samples/repmin.ts",
    "sample:kitchen": "ts-node scripts/verger.ts samples/kitchen.yaml samples/kitchen.ts",
    "sample:spec": "ts-node scripts/verger.ts samples/spec.yaml samples/spec.ts",
    "sample:ir": "ts-node scripts/verger.ts samples/ir.yaml samples/ir.ts",
    "regen": "yarn regen:spec && yarn regen:ir",
    "regen:spec": "ts-node scripts/verger.ts samples/spec.yaml src/specification/nodes.ts",
    "regen:ir": "ts-node scripts/verger.ts samples/ir.yaml src/irepr/nodes.ts",
    "build": "tsc",
    "test": "jest src"
  },
  "keywords": [
    "AST"
  ],
  "files": [
    "lib"
  ],
  "bin": {
    "verger": "./lib/scripts/verger.js"
  },
  "author": "Michael M. Tiller",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^27.0.3",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "clipanion": "^3.1.0",
    "fs-extra": "^10.0.0",
    "prettier": "^2.3.2",
    "purify-ts": "^0.16.3",
    "typanion": "^3.7.1",
    "yaml": "^1.10.2"
  }
}
