{
  "name": "verger",
  "version": "0.1.1",
  "description": "Generation of AST types and helper functions",
  "main": "lib/src/index.js",
  "scripts": {
    "prepublish": "tsc",
    "sample": "yarn sample:expr && yarn sample:repmin && yarn sample:kitchen",
    "sample:expr": "ts-node scripts/verger.ts samples/expr.yaml | tee samples/expr.ts",
    "sample:repmin": "ts-node scripts/verger.ts samples/repmin.yaml | tee samples/repmin.ts",
    "sample:kitchen": "ts-node scripts/verger.ts samples/kitchen.yaml | tee samples/kitchen.ts",
    "sample:dogfood": "ts-node scripts/verger.ts samples/dogfood.yaml | tee samples/dogfood.ts",
    "build": "tsc",
    "test": "jest"
  },
  "keywords": [
    "AST"
  ],
  "files": [
    "lib"
  ],
  "bin": {
    "verger": "./lib/scripts/verger.js"
  },
  "author": "Michael M. Tiller",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^26.0.24",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "clipanion": "^3.0.0-rc.12",
    "fs-extra": "^10.0.0",
    "purify-ts": "^0.16.3",
    "yaml": "^1.10.2"
  }
}
